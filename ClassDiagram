Airport
  - name: String
  + getName(): String
  + setName(name: String): void

Aircraft
  - name: String
  - maxPassengers: int
  + getName(): String
  + setName(name: String): void
  + getMaxPassengers(): int
  + setMaxPassengers(maxPassengers: int): void

Passenger
  - name: String
  - seatNumber: int
  + getName(): String
  + setName(name: String): void
  + getSeatNumber(): int
  + setSeatNumber(seatNumber: int): void